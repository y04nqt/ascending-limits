{
  "name": "ascending-limits",
  "version": "1.0.0",
  "description": "A blog about ascending everyday challenges and learning to make life brighter.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --content-base src --inline --hot",
    "build-css": "node-sass --include-path sass ./css/sass/style.sass   ./css/style.css",
    "watch-css": "nodemon -e sass -x \"npm run build-css\"",
    "build": "webpack -p --progress --config webpack.config.js",
    "dev-build": "webpack --progress -d --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --progress -d --config webpack.config.js --watch"
  },
  "keywords": [
    "blog",
    "climbing",
    "challenges",
    "lifestyle"
  ],
  "author": "Aaron Krueger",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-router": "^4.2.0",
    "webpack": "^3.8.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
